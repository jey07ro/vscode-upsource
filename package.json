{
  "name": "vscode-upsource",
  "displayName": "vscode-upsource",
  "description": "Shows a list of your upsource reviews.",
  "version": "1.0.0",
  "publisher": "kasperbauer",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:upsource.setup",
    "onCommand:upsource.getReviews",
    "onCommand:upsource.allReviews",
    "workspaceContains:upsource.json"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "upsource.setup",
        "title": "Upsource: Setup"
      },
      {
        "command": "upsource.openReviews",
        "title": "Upsource: Open Reviews"
      },
      {
        "command": "upsource.allReviews",
        "title": "Upsource: All Reviews"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@types/request": "0.0.43",
    "opn": "^5.0.0",
    "request": "^2.81.0"
  }
}
